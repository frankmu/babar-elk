{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "chai": true,
    "describe": true,
    "it": true,
    "afterEach": true,
    "beforeEach": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "react/prefer-stateless-function": 1,
    "react/jsx-no-bind": 0,
    "arrow-body-style": 1,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "prefer-rest-params": 0,
    "vars-on-top": 0,
    "react/prop-types": [1, {
      "ignore": ["className"]
    }]
  }
}
